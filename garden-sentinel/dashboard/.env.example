# Garden Sentinel Dashboard Environment Variables

# API URL (leave empty for same-origin, or set for external API)
VITE_API_URL=

# WebSocket URL (optional, defaults to same host)
VITE_WS_URL=

# Map center coordinates (your garden location)
VITE_MAP_CENTER_LAT=0.0
VITE_MAP_CENTER_LNG=0.0
VITE_MAP_DEFAULT_ZOOM=18
